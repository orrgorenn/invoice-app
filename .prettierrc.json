{
    "tabWidth": 2,
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "importOrder": [
      "^(react|next?/?([a-zA-Z/]*))$",
      "<THIRD_PARTY_MODULES>",
      "^@/(.*)$",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports",
      "prettier-plugin-tailwindcss"
    ]
  }